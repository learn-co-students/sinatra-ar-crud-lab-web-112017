<!--  That view should use erb to render the @post's
title and content.-->
<!-- has delete button -->

<h2><%=@post.name%></h2>
<p><%=@post.content%></p>
<form action="/posts/<%=@post.id%>/delete" method="post">
  <input id="hidden" type="hidden" name="_method" value="delete">
  <input id="delete" type=submit value="Delete Post">
</form>
